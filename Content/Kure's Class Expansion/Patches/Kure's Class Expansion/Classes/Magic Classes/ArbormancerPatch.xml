<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<!--Learning Objects Patch-->
	<Operation Class="PatchOperationAddModExtension">
		<xpath>Defs/ThingDef[defName="Torn_BookOfArbormancer"]</xpath>
		<value>
			<li Class="LifeLessons.BillProficiencyExtension">
				<requirements>
					<li>LLRoM_EarthI</li>
					<li>LLRoM_VitalityI</li>
				</requirements>
				<hardRequirement>True</hardRequirement>
			</li>
		</value>
	</Operation>
	<Operation Class="PatchOperationAddModExtension">
		<xpath>Defs/ThingDef[defName="BookOfArbormancer"]</xpath>
